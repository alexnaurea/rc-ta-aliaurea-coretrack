{
  "name": "avolin-coretrac-automation-framework",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "webdriver-manager update --versions.chrome=2.46 --standalone=false --gecko=false",
    "publish:testrail": "aurea-tr-publish COTRAC",
    "clean": "rm -rf auto-generated coverage dist e2e-report *.log",
    "clean:all": "npm run clean -- node_modules",
    "build": "tsc -p ./",
    "lint": "tslint  --project tsconfig.json --config tslint.json",
    "lint:fix": "npm run lint -- --fix",
    "validate": "npm-run-all --parallel build lint",
    "e2e": "protractor protractor.conf.js",
    "retry:prepare": "aurea-retry-prepare",
    "e2e:rerun": "protractor protractor.rerun.conf.js",
    "e2e:ci": "protractor protractor.local.conf.js",
    "e2e:bs": "protractor protractor.browserstack.conf.js",
    "e2e:hub": "protractor protractor.selenium-hub.conf.js",
    "allure:propsadder": "allure-env-properties-adder",
    "docgen": "typedoc --out docs e2e",
    "view:docs": "cd docs && http-server -o",
    "e2e:report": "allure generate auto-generated/allure-results -o e2e-report --clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/compiler": "^6.1.8",
    "@angular/core": "^6.1.8",
    "@aurea/aurea-automation-tslint": "^0.0.3",
    "@aurea/aurea-jasmine-testrail-reporter": "^3.0.0",
    "@aurea/protractor-automation-helper": "0.0.1",
    "allure-commandline": "^2.5.0",
    "allure-env-properties-adder": "^1.0.8",
    "await-handler": "^1.1.2",
    "browserstack-local": "^1.3.0",
    "chai": "^4.2.0",
    "codelyzer": "~4.4.4",
    "jasmine-allure-reporter": "^1.0.2",
    "jasmine-reporters": "^2.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "log4js": "^3.0.6",
    "moment": "^2.23.0",
    "node-vibrant": "^3.0.0",
    "pascal-case": "^2.0.1",
    "protractor": "~5.4.1",
    "request-promise-native": "^1.0.5",
    "rxjs": "^6.3.2",
    "shortid": "^2.2.8",
    "ts-simple-nameof": "^1.1.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-jasmine-rules": "^1.5.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@types/jasmine": "~2.8.7",
    "@types/chai": "^4.1.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~10.11.3",
    "npm-run-all": "^4.1.5",
    "husky": "^1.3.1",
    "jasmine-core": "~3.2.1",
    "lint-staged": "^7.3.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "protractor-parallel-instances": "^1.0.15"
  }
}
